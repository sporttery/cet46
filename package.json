{
  "name": "cet46",
  "version": "1.3.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "debug": "electron . --inspect",
    "winexe": "electron-builder --win  nsis:x64 ",
    "package": "electron-packager ./ CET4 --platform=win32 --arch=x64 --icon=img/app-icon.png --out=./dist --app-version=1.2.0 --overwrite"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^2.0.6",
    "electron-builder": "^22.10.5",
    "electron-packager": "^13.1.1"
  },
  "build": {
    "appId": "com.huanbo.cet46",
    "copyright": "huanbo",
    "productName": "CET46",
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./img/app-icon.ico",
      "uninstallerIcon": "./img/app-icon.ico",
      "installerHeaderIcon": "./img/app-icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "dmg": {
      "window": {
        "x": 100,
        "y": 100,
        "width": 500,
        "height": 400
      }
    },
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "iconv-lite": "^0.6.3",
    "nconf": "^0.11.3",
    "readline": "^1.3.0"
  }
}
